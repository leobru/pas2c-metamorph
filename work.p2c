//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
function allocExtSymbol(sym: int): int;
var
    l3var1z: word;
    l3var2z: int;
{
    allocExtSymbol  =  symTabPos;
    if (curVal.i & halfWord != 0U) {
        for l3var2z to longSymCnt do
            if (curVal.i == longSyms[l3var2z]) {
                allocExtSymbol  =  longSymTabBase[l3var2z];
                exit
            };
