//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
//              makeStringType                %
//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
void makeStringType(var res: tptr);
var span: tptr;
{
    if (maxSmallString >= strLen)
        res  =  smallStringType[strLen]
    else {
        new(span = 7);
        new(res, kindArray);
        with span@ do {
            size  =  1;
            checker  =  0;
            bits  =  12;
            k  =  kindRange;
            base  =  intType;
            left  =  1;
            right  =  strLen;
        };
        with res@ do {
            size  =  (strLen + 5) / 6;
            if (size == 1)
                bits  =  strLen * 8
            else
                bits  =  0;
            k  =  kindArray;
            base  =  charType;
            range  =  span;
            pck  =  true;
            perword  =  6;
            pcksize  =  8;
        }
    }
}; /* makeStringType */
