    form1Insn(i2);
} /* form2Insn */
//
//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
void form3Insn(i1, i2, i3: int);
{
    form2Insn(i1, i2);
    form1Insn(i3);
} /* form3Insn */
