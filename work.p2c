} /* initOptions */
//
{ /* main */
    if (PASINFOR.listMode != 0)
        writeln(boilerplate);
        writeln(' xxx 1');
    initOptions();
        writeln(' xxx 2');
    curInsnTemplate  =  0;
    initTables();
        writeln(' xxx 3');
    programme(curInsnTemplate, hashTravPtr);
    if (errors) {
9999:   writeln(' IN ', (lineCnt-1):0, ' LINES ',
            totalErrors:0, ' ERRORS');
    } else {
        finalize();
        PASINFOR.errors@  =  false;
    }
}
