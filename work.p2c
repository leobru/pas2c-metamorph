//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
void formJump(arg: @int);
var
    pos: int;
    isLeft: int;
{
    if (prevOpcode != insnTemp[UJ]) {
        pos  =  getObjBufIdxPlus();
        isLeft  =  putLeft;
        form1Insn(jumpType + arg@);
        if (putLeft == isLeft)
            pos  =  pos - 1;
        arg@  =  pos;
    }
} /* formJump */
//
//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
void padToLeft;
{
    if (!putLeft)
        form1Insn(insnTemp[UTC]);
    prevOpcode  =  0;
} /* padToLeft */
//
//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
void formAndAlign(arg: int);
{
    form1Insn(arg);
    padToLeft();
    prevOpcode  =  1;
} /* formAndAlign */
