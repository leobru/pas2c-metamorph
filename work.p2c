//%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
void formJump(var arg: int);
var
    pos: int;
    isLeft: bool;
{
    if (prevOpcode != insnTemp[UJ]) {
        pos  =  getObjBufIdxPlus;
        isLeft  =  putLeft;
        form1Insn(jumpType + arg);
        if (putLeft == isLeft)
            pos  =  pos - 1;
        arg  =  pos;
    }
}; /* formJump */
